<!DOCTYPE html>
<html lang="en">
    <head>
        <%- include('../partials/head'); %>
    </head>
    <body class="container">
        <header><%- include('../partials/header'); %></header>

        <main>
            <div class="row">
                <div class="col-sm-8">
                    <img src="<%=image_url%>" />
                </div>
                <div class="col-sm-8">
                    <p>User Name: <%=display_name%></p>
                </div>
                <div class="col-sm-8">
                    <p>Email: <%=email%></p>
                </div>
                <div class="col-sm-8">
                    <p>Views: <%=view_count%></p>
                </div>
                <div class="col-sm-8">
                    <p>Created at: <%=created_at%></p>
                </div>
            </div>
        </main>

        <footer><%- include('../partials/footer'); %></footer>
    </body>
</html>
