{
    "name": "stream-service",
    "version": "1.0.0",
    "private": true,
    "description": "",
    "main": "app.js",
    "scripts": {
        "test": "jest --config ./jest.config.json",
        "test:watch": "npm run test -- --watch",
        "start": "node build/app.js",
        "start:dev": "nodemon src/app.ts",
        "start:debug": "nodemon --watch 'src/**' --ext 'ts,json,js' --ignore 'src/**/*.spec.ts' --exec 'node -r ts-node/register  --inspect -r dotenv/config src/app.ts'",
        "build": "npx tsc -p .",
        "lint": "eslint . --ext .ts",
        "dev:migrate:make": "node -r dotenv/config ./node_modules/knex/bin/cli.js migrate:make --knexfile src/knexfile.ts -x ts",
        "dev:migrate:latest": "node -r dotenv/config ./node_modules/knex/bin/cli.js migrate:latest --knexfile src/knexfile.ts",
        "dev:migrate:rollback": "node -r dotenv/config ./node_modules/knex/bin/cli.js migrate:rollback --knexfile src/knexfile.ts",
        "dev:seed:make": "node -r dotenv/config ./node_modules/knex/bin/cli.js seed:make --knexfile src/knexfile.ts -x ts",
        "dev:seed:run": "node -r dotenv/config ./node_modules/knex/bin/cli.js seed:run --knexfile src/knexfile.ts"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@types/axios": "^0.14.0",
        "@types/cors": "^2.8.8",
        "@types/ejs": "^3.1.0",
        "@types/express": "^4.17.9",
        "@types/express-fileupload": "1.1.6",
        "@types/express-session": "^1.17.4",
        "@types/knex": "^0.16.1",
        "@types/lodash": "^4.14.165",
        "@types/moment": "^2.13.0",
        "@types/moment-timezone": "0.5.30",
        "@types/morgan": "^1.9.2",
        "@types/multer": "1.4.5",
        "@types/node": "^14.14.10",
        "@types/node-cron": "^3.0.0",
        "@types/passport": "^1.0.7",
        "@types/pino": "6.3.5",
        "@types/pump": "1.1.0",
        "@types/request": "^2.48.7",
        "@types/supertest": "^2.0.10",
        "@typescript-eslint/eslint-plugin": "^4.9.0",
        "@typescript-eslint/parser": "^4.9.0",
        "dotenv": "^8.2.0",
        "eslint": "^7.14.0",
        "eslint-config-prettier": "^6.15.0",
        "eslint-plugin-prettier": "^3.2.0",
        "husky": "^4.3.0",
        "jest": "^27.3.1",
        "lint-staged": "^10.5.3",
        "mocha": "^9.1.3",
        "nodemon": "^2.0.6",
        "prettier": "^2.2.1",
        "supertest": "^6.0.1",
        "ts-jest": "^26.4.4",
        "ts-node": "^9.1.0",
        "typescript": "^4.1.2"
    },
    "dependencies": {
        "axios": "^0.21.1",
        "body-parser": "1.19.0",
        "cors": "2.8.5",
        "ejs": "^3.1.6",
        "express": "4.17.1",
        "express-fileupload": "1.2.1",
        "express-session": "^1.17.2",
        "g": "^2.0.1",
        "knex": "^0.95.12",
        "lodash": "^4.17.21",
        "moment": "^2.29.1",
        "moment-timezone": "0.5.32",
        "morgan": "1.10.0",
        "multer": "1.4.2",
        "mysql": "^2.18.1",
        "node": "^14.15.1",
        "node-cron": "^3.0.0",
        "objection": "^2.2.3",
        "passport": "^0.5.0",
        "passport-oauth": "^1.0.0",
        "pino": "6.11.0",
        "request": "^2.88.2"
    },
    "lint-staged": {
        "src/**/*.{js,ts,json,md}": [
            "prettier --write"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    }
}